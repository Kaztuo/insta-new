<% @user.photos.each do |photo| %>
      <div class="col-md-4">
        <%= link_to photo_path(photo.id), remote: true do %>
        <%= image_tag photo.display_image %>
        <% end %>
      </div>
      <div id="photo-modal" class="modal fade" tabindex="-1" role="dialog" aria-hidden="true"></div>
<% end %>