<% if user_signed_in? %>
  <div class="col-md-8">
      <%= render 'shared/feed' %>
  </div>
<% else %>
  <div class="center jumbotron">
    <h1>Instagram アプリ<br>ランディングページ</h1>
    <section class="emaillogin">
      <%= link_to "emailでログイン", new_user_session_url, class: "btn btn-lg btn-primary" %>
    </section>
    
    <section class="facebooklogin">
      <%= link_to "facebookでログイン", user_facebook_omniauth_authorize_path, class: "btn btn-lg btn-primary"  %>
    </section>
  </div>
<% end %>  